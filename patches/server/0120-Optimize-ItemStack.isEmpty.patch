@@ -239,7 +239,7 @@ public final class ItemStack {
     }
 
     public boolean isEmpty() {
-        return this == ItemStack.EMPTY ? true : (this.getItem() != null && !this.is(Items.AIR) ? this.count <= 0 : true);
